    <!-- last items section -->
    <div class="pt-6 overflow-x-auto scrollbars-hidden lg:pt-12 lg:overflow-x-hidden">
      <div class="px-4 inline-flex space-x-2 md:px-8 lg:justify-center lg:grid lg:grid-cols-3 lg:space-x-0 lg:gap-4 lg:px-16 2xl:px-0.5 ">
        <% @items.first(6).each do |item| %>
          <%= link_to item_path(item), class:"" do %>
            <div class="relative pb-2 w-60 rounded-lg lg:w-full border-gray-200 border border-opacity-70 hover:shadow-3xl focus:outline-none">
              <div class="relative h-64 lg:h-72 xl:h-60" style="padding-bottom: 80%">
                <%= cl_image_tag item.photos[0].key, class:'rounded-t absolute h-full w-full inset-0 object-cover shadow'  %>
              </div>
              <div>
                <div class="pt-4 px-2">
                  <div class="truncate text-gray-700 font-semibold tracking-tight font-cursive"><%= item.title %></div>
                </div>
                <% if item.sold? %>
                  <div class=''>
                    <div class="absolute top-0 right-0 p-2">
                      <span class="rounded px-1 py-1 bg-gray-700 text-white text-xs font-bold">Vendu</span>
                    </div>
                    <div class="pt-2 px-2">
                      <span class="font-medium text-gray-800 line-through"> <%= item.price %>€ </span>
                    </div>
                  </div>
                <% else %>
                  <div class="pt-2 px-2">
                    <span class="font-medium text-gray-800"> <%= item.price %>€ </span>
                  </div>
                <% end %>
              </div>
            </div>
          <% end %>
        <% end %>
      </div>
